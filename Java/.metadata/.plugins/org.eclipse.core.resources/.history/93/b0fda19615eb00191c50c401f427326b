package Support;

import java.util.Iterator;

public class ListIterator implements Iterator {
	
	ListNode curNode;
	
	public ListIterator (ListNode node) {
		this.curNode = node;
	}

	@Override
	public boolean hasNext() {
		if (this.curNode.next == null) return true;
		return false;
	}

	@Override
	public Object next() {
		int val = curNode.val;
		curNode = curNode.next;
		return val;
	}

}
