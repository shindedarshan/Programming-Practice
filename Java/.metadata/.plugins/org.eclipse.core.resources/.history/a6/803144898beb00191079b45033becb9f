package queue;

import java.util.Iterator;
import Support.ListNode;

public class QueueIteratorUsingLinkedList implements Iterator {
	
	ListNode start;
	public QueueIteratorUsingLinkedList(ListNode start) {
		this.start = start;
	}

	@Override
	public boolean hasNext() {
		return start != null;
	}

	@Override
	public int next() {
		int data = start.val;
		start = start.next;
		return data;
	}

}
