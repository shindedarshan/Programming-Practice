package QuickUnionFind;

public class QuickUnion {
	int[] list;
	
	public QuickUnion(int n) {
		this.list = new int[n];
		
		for (int i = 0; i < n; i++) this.list[i] = i;
	}
	
	public boolean find(int s, int t) {
		return this.list[s] == this.list[t];
	}
	
	public void union(int s, int t) {
		
	}
}
