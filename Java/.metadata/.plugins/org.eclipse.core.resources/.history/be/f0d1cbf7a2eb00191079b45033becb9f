package sorting;

public class InsertionSort {
	
	public sort(int[] arr) {
		for (int i = 0; i < arr.length; i++) {
			int min = arr[i];
			int minIndex = i;
			for (int j = i; j < arr.length; j++) {
				if (arr[j] < min) {
					minIndex = j;
					min = arr[j];
				}
			}
			int temp = arr[i];
			arr[i] = arr[minIndex];
			arr[minIndex] = arr[i];
		}
	}

	public static void main(String[] args) {
		int[] arr = {4,2,6,3,5,8,9,2,3,4,1,2,3,1,1,8,5,6,4};
	}

}
