package stack;

import java.util.Iterator;

public class ListIteratorUsingArray implements Iterator {
	
	int[] list;
	int n, curr;
	
	public ListIteratorUsingArray(int[] list, int n, int curr) {
		this.list = list;
		this.n = n;
		this.curr = curr;
	}

	@Override
	public boolean hasNext() {
		if (curr < n) return true;
		return false;
	}

	@Override
	public Object next() {
		int val = this.list[this.curr];
		this.curr++;
		return val;
	}

}
