package stack;

public class StackUsingArray {
	
	int[] list;
	int n = -1;
	
	public StackUsingArray () {
		this.list = new int[1];
	}
	
	public void push() {
		
	}
	
	public void pop() {
		
	}
	
	public boolean isEmpty() {
		if (n == -1) return true;
		return false;
	}
	
	public int peek() {
		if (!isEmpty()) return this.list[n];
		else {
			System.out.println("Underflow: Linked list is empty.");
			return -1;
		}
	}
}
